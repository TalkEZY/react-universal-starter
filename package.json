    "dotenv": "^4.0.0",
    "eslint": "^3.14.0",
    "express": "^4.14.0",
    "express-bunyan-logger": "^1.3.1",
    "express-prom-bundle": "^2.2.0",
    "express-react-views": "^0.10.2",
    "express-simple-controllers": "^1.0.1",
    "express-statsd": "~0.3.0",
    "fastclick": "^1.0.6",
    "helmet": "^3.4.0",
    "history": "^4.5.1",
    "match-media": "^0.2.0",
    "match-middleware": "^1.0.2",
    "node-statsd": "^0.1.1",
    "nodemon": "^1.11.0",
    "object-assign": "^4.0.1",
    "postal": "^2.0.4",
    "radium": "^0.18.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "react-helmet": "^5.0.3",
