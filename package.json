{
  "name": "dotcom-video",
  "version": "0.2.0",
  "scripts": {
    "babel": "NODE_ENV=production babel -d dist/server server && NODE_ENV=production babel -d dist/app app",
    "build": "npm run babel && npm run copy && NODE_ENV=production npm run webpack -- --config webpack.config.prod",
    "ci": "NO_WEBPACK_MIDDLEWARE=true ASSET_HOST=http://localhost:8081 node ./bin/devserver",
    "copy": "cp -R server/fixtures dist/server",
    "lint": "eslint app --ext .js,.jsx",
    "prebuild": "rimraf dist/* && rimraf public/assets/*",
    "start:ci": "ASSET_HOST=http://localhost:8081 NO_WEBPACK_MIDDLEWARE=true nodemon --watch server --watch app ./node_modules/.bin/babel-node ./bin/www",
    "start:production": "./bin/www",
    "start": "./node_modules/.bin/babel-node ./bin/www",
    "test": "NODE_ENV=test jest",
    "test:ci": "npm run test -- --watch --notify",
    "test:coverage": "npm run test -- --coverage && open ./coverage/lcov-report/index.html",
    "webpack": "webpack --bail"
  },
  "pre-commit": [
    "test"
  ],
  "dependencies": {
    "airbrake": "^1.2.0",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-polyfill": "^6.5.0",
